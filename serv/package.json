{
  "name": "server",
  "version": "1.0.0",
  "compilerOptions": {
    "esModuleInterop": true
  },
  "scripts": {
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:clean": "npx ts-node ./db/scripts/clean && npm run db:migrate",
    "db:seed": "npm run db:clean && npx ts-node ./db/scripts/seed",
    "db:list": "npx ts-node ./db/scripts/list",
    "db:studio": "blitz prisma studio",
    "db:backup": "npx ts-node ./db/scripts/backup",
    "-------------": "",
    "server:dev:local": "NODE_ENV=dev blitz dev",
    "server:dev:test": "NODE_ENV=test blitz dev",
    "server:dev:prod": "NODE_ENV=prod blitz dev",
    "server:build:local": "NODE_ENV=dev blitz build",
    "server:build:test": "NODE_ENV=test blitz build",
    "server:build:prod": "NODE_ENV=prod blitz build",
    "server:start:local": "NODE_ENV=dev blitz start",
    "server:start:test": "NODE_ENV=test blitz start",
    "server:start:prod": "NODE_ENV=prod blitz start",
    "--------------": "",
    "code:lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "code:test": "vitest run --passWithNoTests",
    "code:test:watch": "vitest",
    "code:prepare": "husky install",
    "---------------": "",
    "extract all messages": "formatjs extract \"src/**/*.ts*\" \"--ignore='**/*.d.ts'\" \"--out-file\" \"lang/en.json\" \"--id-interpolation-pattern\" \"'[sha512:contenthash:base64:6]'\" && npx ts-node ./lang/check" ,
    "compile all messages": "formatjs compile \"lang/en.json\" \"--ast\" \"--out-file\" \"compiled-lang/en.json\" && formatjs compile \"lang/nl.json\" \"--ast\" \"--out-file\" \"compiled-lang/nl.json\" && formatjs compile \"lang/de.json\" \"--ast\" \"--out-file\" \"compiled-lang/de.json\" && formatjs compile \"lang/chi.json\" \"--ast\" \"--out-file\" \"compiled-lang/chi.json\" && formatjs compile \"lang/chi-en.json\" \"--ast\" \"--out-file\" \"compiled-lang/chi-en.json\""
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.289.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.289.0",
    "@aws-sdk/credential-providers": "3.289.0",
    "@blitzjs/auth": "^2.0.0-beta.25",
    "@blitzjs/next": "^2.0.0-beta.25",
    "@blitzjs/rpc": "^2.0.0-beta.25",
    "@playwright/test": "1.32.2",
    "@prisma/client": "^5.2.0",
    "@react-three/drei": "^9.96.5",
    "@react-three/fiber": "^8.15.15",
    "@types/passport-apple": "^1.1.1",
    "@types/passport-google-oauth2": "0.1.5",
    "@types/passport-strategy": "0.2.35",
    "@udecode/zustood": "2.0.0",
    "blitz": "^2.0.0-beta.25",
    "body-parser": "1.20.2",
    "formik": "2.2.9",
    "fuse.js": "6.6.2",
    "gray-matter": "4.0.3",
    "jsonwebtoken": "9.0.2",
    "lodash": "^4.17.21",
    "next": "13.2.4",
    "passport-apple": "2.0.2",
    "passport-facebook": "3.0.0",
    "passport-google-oauth2": "0.2.0",
    "postmark": "3.0.15",
    "qrcode.react": "3.1.0",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-image-gallery": "1.3.0",
    "react-images-uploading": "3.1.7",
    "react-intl": "6.4.7",
    "react-modal": "^3.16.1",
    "react-select": "5.7.0",
    "react-use": "^17.4.0",
    "react-datepicker": "^5.0.0",
    "react-icons": "^5.0.1",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "secure-password": "4.0.0",
    "styled-components": "^5.3.6",
    "three": "0.150.1",
    "uuid": "9.0.1",
    "zod": "3.20.2",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@formatjs/cli": "6.2.0",
    "@next/bundle-analyzer": "12.0.8",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/inquirer": "^9.0.4",
    "@types/jsonwebtoken": "9.0.3",
    "@types/lodash": "4.14.199",
    "@types/node": "18.11.9",
    "@types/passport-facebook": "3.0.1",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.0.25",
    "@types/react-color": "3.0.10",
    "@types/react-image-gallery": "^1.2.1",
    "@types/react-modal": "3.16.3",
    "@types/styled-components": "5.1.28",
    "@types/three": "^0.160.0",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "8.27.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-formatjs": "4.10.5",
    "husky": "8.0.2",
    "inquirer": "^9.2.11",
    "jsdom": "20.0.3",
    "lint-staged": "13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^5.0.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.7",
    "prisma": "^5.2.0",
    "typescript": "^4.8.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "0.25.3"
  },
  "private": true
}
